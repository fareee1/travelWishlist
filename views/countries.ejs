<html>

<head>
  <title>Country List</title>
</head>

<body>
  <h1>List of Countries</h1>
  <ul>
    <% countries.forEach(function(country) { %>
      <li>
        <%= country.name %> - <%= country.alpha2Code %> - <%= country.alpha3Code %> - Visited: <%= country.visited %>
      </li>
      <% }); %>
  </ul>
</body>

</html>